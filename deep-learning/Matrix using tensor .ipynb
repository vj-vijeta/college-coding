import numpy as np
Inv = np.identity(n=m)
Inv= torch.tensor(Inv,dtype=torch.float64)
Inv

mat = np.concatenate((a,Inv),axis=1)
mat =torch.tensor(mat,dtype=torch.float64)
mat

for i in range (0,m):
    pivot = mat[i][i]
    row = mat[i]
    mat[i] = row / pivot
    for j in [k for k in range(0,m) if k!=i]:
        mat[j] = mat[j]-mat[i]*mat[j][i]
    inverse = mat[:,m:]

inverse

