import torch

def determinant(matrix):
    a, b, c, d = matrix[0, 0], matrix[0, 1], matrix[1, 0], matrix[1, 1]
    return ((a * d) - (b * c))

mat = torch.tensor([[3,2],[2,1]],dtype=torch.float64)


det = determinant(mat)
adj = torch.adjoint(mat)

print("Determinant: ", det)

inv = adj/det
inv

